{{#partial "content"}}
    <h1 class="mt-5 text-center">The Squad List</h1>

        {{#each squads}}
            <div class="container mt-5 w-50 list">
                <ul>
                    <li>Squad Name: {{name}}</li>
                    <li>Max Size: {{maxSize}}</li>
                    <li>Cause: {{cause}}/li>
                </ul>
            </div>
        {{/each}}
        <p class="text-center mt-4"> <a href="/squads"><button type="button" class="btn">Add Squad </button></a></p>

{{/partial}}
{{>layout.hbs}}